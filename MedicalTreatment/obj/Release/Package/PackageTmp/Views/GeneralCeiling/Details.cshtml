@model MedicalTreatment.Models.GENERALCEILING

@{
    ViewBag.Title = "Details";
}

<div class="title-header">
    <h4 class="d-flex justify-content-center "><i class="mx-2 my-auto fa fa-hand-holding-medical"></i> السقف العـــــــام</h4>
</div>


<fieldset class="card shadow w-50 p-4" dir="rtl">
    <legend class="bg-custom shadow  px-3">التفاصيل</legend>

    <div class="alert alert-info">
        <b>   السنة العلاجية </b>
        <div class="display-field">
            @Html.DisplayFor(model => model.YEAR)
        </div>
    </div>

    <div class="alert alert-info">
        <b>السقف العلاجي</b>
        <div class="display-field">
            @Html.DisplayFor(model => model.CEILING)
        </div>
    </div>

    <div class="alert alert-info">
        <b>  الحالة</b>
        <div class="display-field">
            @Html.DisplayFor(model => model.STATUS)
        </div>
    </div>

    <div class="alert alert-info">
        <b> اخر تحديث</b>
        <div class="display-field">
            @Html.DisplayFor(model => model.LASTUPDATED)
        </div>
    </div>
</fieldset>
<p class="mt-3 d-flex justify-content-end w-50">
    <a href="/GeneralCeiling/Index" class="btn btn-outline-info mx-2"> رجوع</a>
    @*<a href="/GeneralCeiling/Edit/@Model.ID" type="submit" class="btn btn-success"> تعديل</a>*@
</p>
